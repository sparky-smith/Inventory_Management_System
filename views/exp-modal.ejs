<%- include('include/_header.ejs')%>
<%- include('include/_modalstyle.ejs')%>
</head>

<div class="modal hidden" id="#expandable_form"  >
    <form action="/api/expandable" method="POST">
        <div class="form-group">
            <label for="date">Date</label>
            <input type="date" name="date">
        </div>
        <div class="form-group">
            <label for="">Nomenclature of Expandable Stores</label>
            <input type="text" name="nomenclature_of_expandable">
        </div>
        <div class="form-group">
            <label for="">Quantity</label>
            <input type="number" name="quantity">
        </div>
        <div class="form-group">
            <label for="">Purpose of issue</label>
            <input type="text" name="purpose_of_issue">
        </div>
        <div class="form-group">
            <label for="">Name of Consumer</label>
            <input type="text" name="name_of_consumer">
        </div>
        <div class="form-group">
            <label for="">Remarks</label>
            <input type="text" name="remarks">
        </div>
        <div class="form-group">
            <button type="submit" class="btn">Save</button>
        </div>
    </form>
</div>
<%- include('include/_footer.ejs')%>