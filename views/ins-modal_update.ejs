<%- include('include/_header.ejs')%>
<%- include('include/_modalstyle.ejs')%>
</head>

<div class="modal hidden" >
    <form id="#update_inspection_form" action="/update-inspection/<%= ins._id%>" method="POST">
        <div class="form-group">
            <label for="id">ID</label>
            <input type="text" name="id" value="<%= ins.id%>">
        </div>
        <div class="form-group">
            <label for="">Inspection Date</label>
            <input type="date" name="inspection_date" value="<%= ins.inspection_date%>">
        </div>
        <div class="form-group">
            <label for="">Receipt No</label>
            <input type="text" name="receipt_no" value="<%= ins.receipt_no%>">
        </div>
        <div class="form-group">
            <label for="">Inspection note no</label>
            <input type="text" name="inspect_note_no" value="<%= ins.inspect_note_no%>">
        </div>
        <div class="form-group">
            <label for="">Supply order no</label>
            <input type="text" name="supply_order_no" value="<%= ins.supply_order_no%>">
        </div>
        <div class="form-group">
            <label for="">Supplier</label>
            <input type="text" name="supplier" value="<%= ins.supplier%>">
        </div>
        <div class="form-group"><label for="">Nomenclature</label>
            <input type="text" name="nomenclature" value="<%= ins.nomenclature%>">
        </div>
        <div class="form-group"><label for="">Item name</label>
            <input type="text" name="item_name" value="<%= ins.item_name%>">
        </div>
        <div class="form-group"><label for="">Issued person</label>
            <input type="text" name="issued_person" value="<%= ins.issued_person%>">
        </div>
        <div class="form-group"><label for="">Issued person's section</label>
            <input type="text" name="issued_person_section" value="<%= ins.issued_person_section%>">
        </div>
        <div class="form-group">
            <button type="submit" class="btn">Update</button>
        </div>
    </form>
</div>
<%- include('include/_footer.ejs')%>
